              <term>Vertical Label</term>
            </item>
            <item>
              <term>Button</term>
            </item>
            <item>
              <term>Hyperlink</term>
            </item>
            <item>
              <term>Scrolling Region</term>
            </item>
            <item>
              <term>Choice Group</term>
            </item>
            <item>
              <term>Repeating Choice Group</term>
            </item>
            <item>
              <term>Choice Section</term>
            </item>
