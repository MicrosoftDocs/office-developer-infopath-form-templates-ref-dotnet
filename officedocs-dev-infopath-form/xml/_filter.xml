    <apiFilter apply="true">
        <namespaceFilter name="Microsoft.Office.InfoPath" expose="true">
            <typeFilter name="AdoQueryConnection" expose="false"/>
            <typeFilter name="AdoSubmitConnection" expose="false"/>
            <typeFilter name="Application" expose="false"/>
            <typeFilter name="BdcQueryConnection" expose="false"/>
            <typeFilter name="BdcSubmitConnection" expose="false"/>
